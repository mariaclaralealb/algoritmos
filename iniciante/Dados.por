programa {
  funcao inicio() {
    
    //Declaração de variavéis
    cadeia n
    real i
    real c
    real t

    //Entrada
    escreva("Digite seu Nome:")
    leia(n)
    escreva("Digite sua idade:")
    leia(i)
    escreva("Digite seu cpf:")
    leia(c)
    escreva("Digite seu telefone:")
    leia(t)

    //Saída
    escreva("nome: ",n,"\nidade: ", i, "\ncpf: ",c,"\ntelefone: ",t )
    
  }
}
