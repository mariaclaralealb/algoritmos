programa {
  funcao inicio() {

    //Declaração de variavéis
    real n1, n2, area

    //Entrada
    escreva("Digite o comprimento do terreno:")
    leia(n1)
    escreva("Digite a largura do terreno:")
    leia(n2)

    //Processamento
    area= n1 * n2

    //Saída
    escreva("A area do terreno é :", area) 

    
  }
}
