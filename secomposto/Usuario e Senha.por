programa {
  funcao inicio() {
    cadeia  usuario, senha

    escreva("Digite seu Usuario:")
    leia(usuario)
    se (usuario == "M")
    escreva (/n"Usuario Correto")
    senao 
    escreva (/n"Usuario Incorreto")

    escreva("Digite sua senha:")
    leia(senha)
    se (senha == "maria")
    escreva ("Senha Correta")
    senao
    escreva ("Senha Incorreta")

   



    
  }
}
