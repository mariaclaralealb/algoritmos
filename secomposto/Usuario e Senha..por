programa {
  funcao inicio() {
    cadeia  usuario, senha

    escreva("Digite seu Usuario:")
    leia(usuario)

    se (usuario == "usuario")
    escreva ("\nUsuario Correto")
    senao 
    escreva ("\nUsuario Incorreto")

    escreva("\nDigite sua senha:")
    leia(senha)

    se (senha == "usuario")
    escreva ("\nSenha Correta")
    senao
    escreva ("\nSenha Incorreta")
    
  }
}
